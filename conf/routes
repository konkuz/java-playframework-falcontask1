# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.ImplAppController.index()
POST    /jsonMessage                controllers.ImplJSONController.addJSONMessage()
GET     /jsonMessages               controllers.ImplJSONController.retrieveJSONMessages()
GET     /websocket                  controllers.ImplWebSocketController.runWebsocket()
GET     /websocketMessages          controllers.ImplWebSocketController.index()

#previous solution
POST    /person                     controllers.ImplPersonController.addPerson()
GET     /persons                    controllers.ImplPersonController.retrievePersons()